  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.147",
    "@types/node": "^12.12.14",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
